{
    "name": "takuya/laravel-plugin-nulab-backlog-archiver",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "takuya/php-nulab-backlog-api-client": "master"
    },
    "require-dev": {
        "laravel/lumen-framework": ">=9.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Takuya\\Laravel\\Backlog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Takuya\\Laravel\\Backlog\\Providers\\AppServiceProvider"
            ]
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "php-nulab-backlog-api-client": {
            "type": "vcs",
            "url": "https://github.com/takuya/php-nulab-backlog-api-client"
        }
    }
}
